<% if data_bag_id -%>
<h1><%= data_bag_id %></h1>
<% end -%>
<% if @search_query -%>
<%   if resources.empty? -%>
<p class="lead">No matching results found.</p>
<%   else -%>
<p class="lead">Search results (<%= resources.length %>)</p>
<%   end -%>
<% end -%>
<ul class="unstyled">
<% resources.each do |resource| -%>
  <li><a href='<%= url resource.url %>'><%= resource.chef_id %></a></li>
<% end -%>
</ul>
